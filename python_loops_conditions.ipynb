        "# Loops and Conditions\n",
        "\n",
        "# For loop\n",
        "for i in range(5):\n",
        "    print(\"Number:\", i)\n",
        "\n",
        "# While loop\n",
        "count = 0\n",
        "while count < 3:\n",
        "    print(\"Count:\", count)\n",
        "    count += 1\n",
        "\n",
        "# If-Else\n",
        "num = 7\n",
        "if num % 2 == 0:\n",
        "    print(num, \"is Even\")\n",
        "else:\n",
        "    print(num, \"is Odd\")"
